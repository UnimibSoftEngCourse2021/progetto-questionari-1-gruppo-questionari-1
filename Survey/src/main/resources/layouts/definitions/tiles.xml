<tiles-definitions>
	<!-- start base layout -->

	<definition name="baseLayout" template="/layouts/template/base.jsp">
		<put-attribute name="content" value="" />
	</definition>

	<definition name="navBarNotLogged" template="/layouts/template/navBarNotLogged.jsp">
		<!--<put-attribute name="loginButton" value="" /> 
		<put-attribute name="registerButton" value="" />-->
		<put-attribute name="notLoggedButtons" value="" />
	</definition>

	<!-- end base layout-->

	<!-- start index layout -->

	<definition name="index" template = "/viste/index.jsp" >
		<put-attribute name="head" value="/layout/template/head.jsp" />
		<put-attribute name="navBarNotLogged" value="/viste/navBarNotLogged.jsp" /> 
		<put-attribute name="masthead" value="/template/masthead.jsp" />
		<put-attribute name="scripts" value="/viste/scripts.jsp" /> 


	</definition>

	<!-- end index layout--> 
	
	<!--start question layout-->
	<definition name="questions" template = "/viste/questions.jsp" >
		<put-attribute name="head" value="/layout/template/head.jsp" />
		<put-attribute name="navBarLogged" value="/template/navBarLogged.jsp" /> 
		<put-attribute name="containerDomande" value="/template/containerDomande.jsp" />
		<put-attribute name="scripts" value="/template/scripts.jsp" /> 


	</definition>

<!--end questions layout-->


<!--start create Survey layout-->
	
	<definition name="createSurvey" template = "/viste/createSurvey.jsp" >
		<put-attribute name="head" value="/layout/template/head.jsp" />
		<put-attribute name="navBarLogged" value="/template/navBarLogged.jsp" /> 
		
		<put-attribute name="scripts" value="/template/scripts.jsp" /> 


	</definition>

<!--end create Survey layout-->

<!--start manage Survey layout-->
	
	<definition name="manageSurvey" template = "/viste/manageSurvey.jsp" >
		<put-attribute name="head" value="/layout/template/head.jsp" />
		<put-attribute name="navBarLogged" value="/template/navBarLogged.jsp" /> 
		
		<put-attribute name="scripts" value="/template/scripts.jsp" /> 


	</definition>

<!--end manage Survey layout-->

<!--start compiled Survey layout-->
	
	<definition name="compiledSurvey" template = "/viste/compiledSurvey.jsp" >
		<put-attribute name="head" value="/layout/template/head.jsp" />
		<put-attribute name="navBarLogged" value="/template/navBarLogged.jsp" /> 
		
